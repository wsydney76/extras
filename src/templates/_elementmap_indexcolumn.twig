{%- do view.registerAssetBundle("wsydney76\\extras\\web\\assets\\elementmap\\ElementMapAsset") -%}

<div class="elementmap-table">
    {% if elements is iterable %}
        <ul>
            {% for element in elements %}
                <li>
                    <a target="_blank" href="{{ element.url }}" data-id="{{ element.id }}" data-editable>
                        {#{% if element.icon is defined %}
                            <span class="icon icon-mask">{{ svg(element.icon) }}</span>
                        {% endif %}#}
                        {{ element.title }}</a>
                </li>
            {% endfor %}
        </ul>
    {% elseif elements %}
        {{ elements }}
    {% else %}
        <span>--</span>
    {% endif %}

</div>
